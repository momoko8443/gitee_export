{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.config.js",
    "packageDarwin": "electron-packager . electron --platform=darwin --arch=x64 --out=./target --asar --app-version=1.0.0",
    "packageWin": "electron-packager . electron --platform=win32 --arch=x64  --out=./target --asar --app-version=1.0.0"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "electron-fetch": "^1.3.0",
    "exceljs": "^1.7.0",
    "node-loader": "^0.6.0",
    "source-map-loader": "^0.2.4",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "devDependencies": {
    "electron": "^4.0.7",
    "electron-packager": "^13.1.1"
  }
}
